@{
    ViewData["Title"] = "Index";
}


@model List<Category>
<a asp-controller="Category" asp-action="Create" class="btn btn-success mb-2">Create</a>
<table class="table table-dark">
    <tr>
        <th>Id</th>
        <th>Name</th>
    </tr>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
            </tr>
        }
    </tbody>
</table>